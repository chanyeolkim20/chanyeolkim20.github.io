      "source": [
        "f = lambda x: np.sin(x)\n",
        "error = 100\n",
        "n = 1\n",
        "while error > 10**-10:\n",
        "  x = np.linspace(0, np.pi/2, num=n, endpoint=True)\n",
        "  poly = np.polyfit(x, f(x), n-1)\n",
        "  integral = np.poly1d(np.polyint(poly))\n",
        "  est = integral(np.pi/2) - integral(0)\n",
        "  error = np.abs(est - ans)\n",
        "  print(\"estimate = {}\".format(est))\n",
        "  print(\"error = {}\".format(error))\n",
        "  n+=1\n",
        "\n",
        "print(\"number of gridpoints = \", len(x))\n",
        "\n",
        "x_hres = np.linspace(0, np.pi/2, num=1001, endpoint=True)\n",
        "plt.plot(x_hres, f(x_hres), label=\"original\")\n",
        "plt.plot(x, np.poly1d(poly)(x), label=\"polynomial\")\n",
        "plt.scatter(x, np.poly1d(poly)(x), c=\"black\", label=\"grid points\")\n",
        "plt.title(\"Plotting sin x, n res polynomial, and explicit grid points\")\n",
        "plt.xlabel(\"x\")\n",
        "plt.ylabel(\"y\")\n",
        "plt.legend()"
